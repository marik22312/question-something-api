    "jest": "^24.9.0",
    "mongodb-memory-server": "^6.1.1",
    "supertest": "^4.0.2",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "mongoose": "^5.8.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
